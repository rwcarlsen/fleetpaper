@article{kim_selection_1999,
	title = {Selection of an optimal nuclear fuel cycle scenario by goal programming and the analytic hierarchy process},
	volume = {26},
	issn = {03064549},
	doi = {10.1016/S0306-4549(98)00081-4},
	abstract = {The front-end fuel cycle from mining to enrichment has reached maturity. Unlike the front-end fuel cycle, there are several pathways in the back-end fuel cycle. The long-term consequences of any decision for the back-end fuel cycle requires some sophisticated decision making tools. Various kinds of factors should be taken into consideration for the decision. In this study the Goal Programming Method in combination with the Analytic Hierarchy Process ({AHP}) is applied in the decision making process. For the treatment of tangible factors Goal Programming is used and for intangible factors {AHP} is used for quantification. A computer code was developed to combine both methods in the treatment of two different kinds of factors and to obtain a solution for the most optimal fuel cycle in Korea.},
	number = {5},
	journal = {Annals of Nuclear Energy},
	author = {Kim, Poong Oh and Lee, Kun Jai and Lee, Byong Whi},
	year = {1999},
	keywords = {Computer software, Decision making, Mathematical programming, Nuclear fuel reprocessing},
	pages = {449--460}
}

@inproceedings{yacout_agent_2012,
	series = {Transactions of the American Nuclear Society},
	title = {Agent based simulation of the nuclear fuel cycle},
	volume = {106},
	isbn = {0003018X},
	abstract = {The current version of {SINDA} is a Java dynamic simulation code that utilizes an agent-based development approach. Its current capabilities are similar to those of a typical fuel cycle system dynamics simulation code such as the {DYMOND} code which include abilities to perform future scenarios of nuclear fuel cycle infrastructure. In addition, the code can provide flexibility for fuel cycle scenarios options (e.g., more reactors and fuel types as well as multi-regional simulations) and its modular design allows for calling other process models or physics codes as needed. In general, the use of Java as the development programming language allows developers to add many more options to the code than with classical systems dynamics environments. Future code implementations include following materials flow at the isotopic level and allowing for its decay at key areas of the fuel cycle. Agent representations will be expanded to provide the option for detailed tracking at the level of single structure (e.g., tracking single reactor of specific type rather than tracking of a group of reactors of specific type if needed). The autonomy of certain types of agents (e.g. reprocessing facilities) will be expanded providing it with rules that allow it to function as independent agent that can modify its characteristics as needed. This capability can be a key feature for the purpose of performing certain optimization schemes that can be called for by the code user.},
	booktitle = {2012 {ANS} Annual Meeting and Embedded Topical Meeting: Nuclear Fuels and Structural Materials for the Next Generation Nuclear Reactors, {NFSM} 2012, June 24, 2012 - June 28, 2012},
	publisher = {American Nuclear Society},
	author = {Yacout, A.M. and Blanchard, G. and Taiwo, T.A.},
	year = {2012},
	keywords = {Autonomous agents, Computer simulation, Nuclear fuel reprocessing, Nuclear fuels, Nuclear reactors},
	pages = {249--251}
}

@article{juchau_modeling_2010,
	title = {Modeling the nuclear fuel cycle},
	volume = {171},
	issn = {00295450},
	abstract = {A review of existing analysis codes for nuclear fuel cycle systems was performed to determine if any existing codes meet technical and functional requirements de fined for a U.S. national program supporting the global and domestic assessment, development, and deployment of nuclear energy systems. The program would be imple mented using an interconnected architecture of d codes ranging from the fuel cycle analysis code, which is the subject of the review, to fundamental physical and mechanistic codes. Four main functions are defined for the code. Function 1 is the ability to characterize and deploy individual fuel cycle facilities and reactors in a simulation while discretely tracking material move ments. Function 2 is the capability to perform an uncer tainty analysis for each element of the fuel cycle and an aggregate uncertainty analysis. Function 3 is the inclu sion of an optimization engine able to optimize simulta neously across multiple objective functions. Function 4 is open and accessible code software and documentation to aid in collaboration between multiple entities and to facilitate software updates. Existing codes, categorized as annualized or dis crete fuel tracking codes, were assessed according to the fourfunctions and associated requirements. These codes were developed by various government, education, and industrial entities to fulfill particular needs. In some cases, decisions were made during code development to limit the level of detail included in a code to ease its use or to focus on certain aspects of a fuel cycle to address specific questions. The review revealed that while no two of the codes are identical, they all perform many of the same basic functions. No code was able to perform defined function 2 or several requirements of functions 1 and 3. Based on this review, it was concluded that the functions and requirements will be met only with development of a new code, referred to as {GENIUS}.},
	number = {2},
	journal = {Nuclear Technology},
	author = {Juchau, Christopher A. and Dunzik-Gougar, Mary Lou and Jacobson, Jacob J.},
	year = {2010},
	keywords = {Fuels, Nuclear energy, Nuclear fuel reprocessing, Optimization, Supports, Uncertainty analysis},
	pages = {136--141}
}

